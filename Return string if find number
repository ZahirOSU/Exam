char* findNumber(int arr_count, int* arr, int k) {

for(int i = 0 ; i < arr_count ; i++)
{
    if(arr[i]==k) 
    return "YES"; 
    
       
}
 
return "NO";
}
int main(){
    char *s;
    int arr_count =  3;
    int arr[3] = {1,2,3};
    int  k = 1 ;
    s = findNumber( arr_count, arr,  k);
  
  printf("%s", s);
    
    
}

// if number in the range return yes if not return no.
